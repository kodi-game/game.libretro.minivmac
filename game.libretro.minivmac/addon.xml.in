<?xml version="1.0" encoding="UTF-8"?>
<addon id="game.libretro.minivmac"
		name="minivmac"
		version="0.0.1.1"
		provider-name="Libretro">
	<requires>
		<import addon="game.libretro" version="1.0.0"/>
	</requires>
	<extension point="kodi.gameclient"
			library_@PLATFORM@="@LIBRARY_FILENAME@">
		<platforms></platforms>
		<extensions>dsk|img|zip|hvf|cmd</extensions>
		<supports_vfs>false</supports_vfs>
		<supports_standalone>false</supports_standalone>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en_GB">minivmac b36</summary>
		<description lang="en_GB">Supported files: .dsk, .img, .zip, .hvf, .cmd</description>
		<license></license>
		<platform>@PLATFORM@</platform>
		<source>https://github.com/kodi-game/game.libretro.minivmac</source>
	</extension>
</addon>
